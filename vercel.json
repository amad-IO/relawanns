{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}